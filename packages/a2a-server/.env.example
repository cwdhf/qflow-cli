# =============================================================================
# Server Configuration
# =============================================================================
CODER_AGENT_PORT=41242

GEMINI_API_KEY=your-gemini-api-key
CHECKPOINTING=false

# 腾讯云 COS 文件上传配置 (可选)
# COS_SECRET_ID=your-cos-secret-id
# COS_SECRET_KEY=your-cos-secret-key
# COS_BUCKET_NAME=your-bucket-name
# COS_REGION=ap-beijing
# COS_TARGET_DIR=your-target-dir/

# Debug Configuration
# DEBUG=true
# GEMINI_STREAM_LOGGING=true
# GEMINI_DEBUG_LOG_FILE=./logs/debug.log

# Telemetry Configuration (disabled for China network)
# TELEMETRY=false
# GEMINI_TELEMETRY_DISABLED=true

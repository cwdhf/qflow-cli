# =============================================================================
# OpenAI兼容模型配置说明
# 支持阿里云、火山云、月之暗面等多个服务商的模型
# =============================================================================

# -----------------------------------------------------------------------------
# 方案一：阿里云 DashScope
# -----------------------------------------------------------------------------
# 模型配置
OPENAI_DEFAULT_MODEL=glm-4.7
OPENAI_DEFAULT_EMBEDDING_MODEL=text-embedding-v4
OPENAI_MODELS=[{"value":"glm-4.7","title":"GLM-4.7","description":"智谱GLM-4.7模型","key":"glm-4.7"},{"value":"deepseek-v3.2","title":"DeepSeek-V3.2","description":"DeepSeek V3.2 稀疏注意力模型","key":"deepseek-v3.2"},{"value":"qwen3-max-preview","title":"Qwen3-Max-Preview","description":"通义千问Qwen3预览版","key":"qwen3-max-preview"},{"value":"kimi-k2-thinking","title":"Kimi-K2-Thinking","description":"月之暗面Kimi K2思考模型","key":"kimi-k2-thinking"}]
OPENAI_MODEL_ALIASES={"standard":"qwen3-max-preview","turbo":"deepseek-v3.2","pro":"glm-4.7","multimodal":"qwen3-max-preview"}

# API配置
OPENAI_BASE_URL=https://dashscope.aliyuncs.com/compatible-mode/v1
OPENAI_API_KEY=sk-your-aliyun-api-key

